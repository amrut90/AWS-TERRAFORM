#create a S3 bucket

provider "aws" {
  access_key = "******************"
  secret_key = "*********************"
  region     = "**************"
}

#define s3 bucket

resource "aws_s3_bucket" "bucket" {
  bucket = "**************"
  acl    = "private"
  versioning {
    enabled = true
  }
  tags = {
    Name        = "*************"
    Environment = "Dev"
  }
}
